{
  "expo": {
    "name": "Anti-Matter",
    "slug": "anti-matter",
    "scheme": "vibecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.vibecode.anti-matter-nwkjjf",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Anti-Matter uses the camera to capture photos inside the app before you publish them.",
        "NSPhotoLibraryUsageDescription": "Anti-Matter accesses your photo library to let you choose media that appears in the app.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./icon.png"
  }
}
